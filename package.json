{
  "name": "eslinter",
  "version": "1.0.0",
  "description": "Eslinter",
  "main": "index.js",
  "scripts": {
    "try": "eslint .",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "test": "jest",
    "watch": "watch 'clear && npm run -s test | tap-nirvana && npm run -s lint' src",
    "watch:win": "watch \"cls && npm run -s test | tap-nirvana && npm run -s lint\" src"
  },
  "keywords": [
    "Eslint"
  ],
  "author": "Ivan Yew",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "tap-nirvana": "^1.1.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "react": "^16.8.6"
  },
  "jest": {
    "roots": [
      "test"
    ]
  }
}
