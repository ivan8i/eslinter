{
  "name": "eslinter",
  "version": "1.0.0",
  "description": "Eslinter",
  "main": "index.js",
  "scripts": {
    "try": "eslint .",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "test": "jest",
    "watch": "watch 'clear && npm run -s test | tap-nirvana && npm run -s lint' src",
    "watch:win": "watch \"cls && npm run -s test | tap-nirvana && npm run -s lint\" src"
  },
  "keywords": [
    "Eslint"
  ],
  "author": "Ivan Yew",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "tap-nirvana": "^1.1.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "jest": {
    "roots": [
      "test"
    ]
  }
}
