{
  "name": "eslinter",
  "version": "1.0.0",
  "description": "Eslinter",
  "main": "index.js",
  "scripts": {
    "try": "eslint .",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "test": "jest",
    "watch": "watch 'clear && npm run -s test | tap-nirvana && npm run -s lint' src",
    "watch:win": "watch \"cls && npm run -s test | tap-nirvana && npm run -s lint\" src"
  },
  "keywords": [
    "Eslint"
  ],
  "author": "Ivan Yew",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "tap-nirvana": "^1.1.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "react": "^16.13.1"
  },
  "jest": {
    "roots": [
      "test"
    ]
  }
}
